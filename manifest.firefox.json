{
    "manifest_version": 2,
    "name": "RoMonitor Stats - Roblox Stats",
    "author": "Silicon Digital Group Ltd",
    "version": "1.0.4",
    "description": "View Roblox game analytics from RoMonitor Stats inside Roblox",
    "content_scripts": [
      {
        "js": [
          "stats.js"
        ],
        "css": [
          "romonitor.css"
        ],
        "matches": [
          "*://*.roblox.com/games/*",
          "*://*.roblox.com/users/*",
          "*://*.roblox.com/groups/*",
          "https://romonitorstats.com/api/v1/extension/*"
        ]
      }
    ],
    "permissions": [
      "*://*.roblox.com/games/*",
      "*://*.roblox.com/users/*",
      "*://*.roblox.com/groups/*",
      "https://romonitorstats.com/api/v1/extension/*",
      "webRequest"
    ],
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }